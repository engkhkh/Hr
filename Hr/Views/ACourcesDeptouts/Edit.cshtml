@model Hr.Models.ACourcesDeptout

@{
    ViewData["Title"] = "تعديل";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>تعديل</h1>

@*<h4>ACourcesDeptout</h4>*@
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="CourcesIdDeptout" />
            <div class="form-group">
                @*<label asp-for="CourcesNameDeptout" class="control-label"></label>*@
                <input asp-for="CourcesNameDeptout" class="form-control" />
                <span asp-validation-for="CourcesNameDeptout" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="حفظ" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">عودة الي الرئيسية</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
