@model IEnumerable<Hr.Models.ACourcesEstimate>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>

<table class="table">
    <thead>
        <tr>
            <th></th>
            <th>
                @Html.DisplayNameFor(model => model.CourcesNameEstimate)
            </th>

        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>

                <a asp-action="Create">اضافة </a>|
                <a asp-action="Edit" asp-route-id="@item.CourcesIdEstimate">تعديل</a> |
                <a asp-action="Details" asp-route-id="@item.CourcesIdEstimate">تفاصيل </a> |
                @*<a asp-action="Delete" asp-route-id="@item.CourcesIdEstimate">Delete</a>*@
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CourcesNameEstimate)
            </td>

        </tr>
}
    </tbody>
</table>
</body>
</html>
