@model IEnumerable<Hr.Models.ViewModelEvalwithother>

@{
    ViewData["Title"] = "تحميل درجة المقابلات الشخصية ";
    //Layout = "~/Views/Shared/_Layout - Copy - Copy.cshtml";
    Layout = "~/Views/Shared/_Layout.cshtml";
